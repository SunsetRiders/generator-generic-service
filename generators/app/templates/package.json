    "moment":              "^2.15.2",
    "pg":                  "^6.1.0",
    "request":             "^2.78.0",
    "request-promise":     "^4.1.1",
    "winston":             "^2.2.0",
    "winston-logentries":  "^2.0.0"
  }
}
